{
  "name": "@manifold/cli",
  "version": "2.14.4",
  "description": "Native CLI for Manifold constraint-first development framework",
  "type": "module",
  "bin": {
    "manifold": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build ./index.ts --outdir ./dist --target bun",
    "compile": "bun build ./index.ts --compile --outfile ./manifold",
    "compile:all": "bun run compile:darwin-arm64 && bun run compile:darwin-x64 && bun run compile:linux-x64 && bun run compile:linux-arm64",
    "compile:darwin-arm64": "bun build ./index.ts --compile --target=bun-darwin-arm64 --outfile ./dist/manifold-darwin-arm64",
    "compile:darwin-x64": "bun build ./index.ts --compile --target=bun-darwin-x64 --outfile ./dist/manifold-darwin-x64",
    "compile:linux-x64": "bun build ./index.ts --compile --target=bun-linux-x64 --outfile ./dist/manifold-linux-x64",
    "compile:linux-arm64": "bun build ./index.ts --compile --target=bun-linux-arm64 --outfile ./dist/manifold-linux-arm64",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "manifold",
    "constraints",
    "cli",
    "development"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhanesh/manifold"
  }
}
