{
  "$ref": "#/definitions/ManifoldStructure",
  "definitions": {
    "ManifoldStructure": {
      "type": "object",
      "properties": {
        "$schema": {
          "type": "string"
        },
        "schema_version": {
          "type": "number",
          "const": 3,
          "default": 3
        },
        "feature": {
          "type": "string",
          "minLength": 1
        },
        "phase": {
          "type": "string",
          "enum": [
            "INITIALIZED",
            "CONSTRAINED",
            "TENSIONED",
            "ANCHORED",
            "GENERATED",
            "VERIFIED"
          ]
        },
        "mode": {
          "type": "string",
          "enum": [
            "light",
            "full"
          ]
        },
        "outcome_ref": {
          "type": "string"
        },
        "template": {
          "type": "string"
        },
        "template_version": {
          "type": "number"
        },
        "created": {
          "type": "string"
        },
        "updated": {
          "type": "string"
        },
        "constraints": {
          "type": "object",
          "properties": {
            "business": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^[BTUSO]\\d+$"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "invariant",
                      "goal",
                      "boundary"
                    ]
                  }
                },
                "required": [
                  "id",
                  "type"
                ],
                "additionalProperties": false
              },
              "default": []
            },
            "technical": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^[BTUSO]\\d+$"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "invariant",
                      "goal",
                      "boundary"
                    ]
                  }
                },
                "required": [
                  "id",
                  "type"
                ],
                "additionalProperties": false
              },
              "default": []
            },
            "user_experience": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^[BTUSO]\\d+$"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "invariant",
                      "goal",
                      "boundary"
                    ]
                  }
                },
                "required": [
                  "id",
                  "type"
                ],
                "additionalProperties": false
              },
              "default": []
            },
            "security": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^[BTUSO]\\d+$"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "invariant",
                      "goal",
                      "boundary"
                    ]
                  }
                },
                "required": [
                  "id",
                  "type"
                ],
                "additionalProperties": false
              },
              "default": []
            },
            "operational": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^[BTUSO]\\d+$"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "invariant",
                      "goal",
                      "boundary"
                    ]
                  }
                },
                "required": [
                  "id",
                  "type"
                ],
                "additionalProperties": false
              },
              "default": []
            }
          },
          "additionalProperties": false
        },
        "tensions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "pattern": "^TN\\d+$"
              },
              "type": {
                "type": "string",
                "enum": [
                  "trade_off",
                  "resource_tension",
                  "hidden_dependency"
                ]
              },
              "between": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "minItems": 2
              },
              "status": {
                "type": "string",
                "enum": [
                  "resolved",
                  "unresolved"
                ]
              }
            },
            "required": [
              "id",
              "type",
              "between",
              "status"
            ],
            "additionalProperties": false
          },
          "default": []
        },
        "tension_summary": {
          "type": "object",
          "properties": {
            "trade_offs": {
              "type": "number"
            },
            "resource_tensions": {
              "type": "number"
            },
            "hidden_dependencies": {
              "type": "number"
            },
            "total": {
              "type": "number"
            },
            "resolved": {
              "type": "number"
            },
            "unresolved": {
              "type": "number"
            }
          },
          "required": [
            "trade_offs",
            "resource_tensions",
            "hidden_dependencies",
            "total",
            "resolved",
            "unresolved"
          ],
          "additionalProperties": false
        },
        "anchors": {
          "type": "object",
          "properties": {
            "required_truths": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^RT-\\d+$"
                  },
                  "status": {
                    "type": "string",
                    "enum": [
                      "SATISFIED",
                      "PARTIAL",
                      "NOT_SATISFIED",
                      "SPECIFICATION_READY"
                    ]
                  },
                  "maps_to": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "required": [
                  "id",
                  "status"
                ],
                "additionalProperties": false
              },
              "default": []
            },
            "recommended_option": {
              "type": "string"
            },
            "implementation_phases": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        },
        "iterations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "number": {
                "type": "number"
              },
              "phase": {
                "type": "string"
              },
              "timestamp": {
                "type": "string"
              },
              "result": {
                "type": "string"
              }
            },
            "required": [
              "number",
              "phase",
              "timestamp",
              "result"
            ],
            "additionalProperties": true
          }
        },
        "convergence": {
          "type": "object",
          "properties": {
            "status": {
              "type": "string",
              "enum": [
                "NOT_STARTED",
                "IN_PROGRESS",
                "CONVERGED"
              ]
            },
            "criteria": {
              "type": "object",
              "properties": {
                "all_invariants_satisfied": {
                  "type": "boolean"
                },
                "all_required_truths_satisfied": {
                  "type": "boolean"
                },
                "no_blocking_gaps": {
                  "type": "boolean"
                },
                "all_integrations_complete": {
                  "type": "boolean"
                },
                "strict_verification_passed": {
                  "type": "boolean"
                }
              },
              "additionalProperties": false
            },
            "iterations_to_convergence": {
              "type": "number"
            },
            "timestamp": {
              "type": "string"
            }
          },
          "required": [
            "status"
          ],
          "additionalProperties": false
        },
        "generation": {
          "type": "object",
          "properties": {
            "option": {
              "type": "string"
            },
            "timestamp": {
              "type": "string"
            },
            "iteration": {
              "type": "number"
            },
            "artifacts": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "path": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string"
                  },
                  "satisfies": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "status": {
                    "type": "string"
                  }
                },
                "required": [
                  "path",
                  "type",
                  "status"
                ],
                "additionalProperties": false
              }
            },
            "coverage": {
              "type": "object",
              "properties": {
                "constraints_addressed": {
                  "type": "number"
                },
                "constraints_total": {
                  "type": "number"
                },
                "required_truths_addressed": {
                  "type": "number"
                },
                "required_truths_total": {
                  "type": "number"
                },
                "percentage": {
                  "type": "number"
                }
              },
              "required": [
                "constraints_addressed",
                "constraints_total",
                "required_truths_addressed",
                "required_truths_total",
                "percentage"
              ],
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "evidence": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "enum": [
                  "file_exists",
                  "content_match",
                  "test_passes",
                  "metric_value",
                  "manual_review"
                ]
              },
              "path": {
                "type": "string"
              },
              "pattern": {
                "type": "string"
              },
              "test_name": {
                "type": "string"
              },
              "metric_name": {
                "type": "string"
              },
              "threshold": {
                "type": [
                  "string",
                  "number"
                ]
              },
              "status": {
                "type": "string",
                "enum": [
                  "VERIFIED",
                  "PENDING",
                  "FAILED",
                  "STALE"
                ]
              },
              "satisfies": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "id",
              "type"
            ],
            "additionalProperties": false
          }
        },
        "constraint_graph": {
          "type": "object",
          "properties": {
            "version": {
              "type": "number"
            },
            "feature": {
              "type": "string"
            },
            "generated_at": {
              "type": "string"
            },
            "nodes": {
              "type": "object",
              "additionalProperties": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "constraint",
                      "tension",
                      "required_truth",
                      "artifact"
                    ]
                  },
                  "status": {
                    "type": "string",
                    "enum": [
                      "UNKNOWN",
                      "REQUIRED",
                      "SATISFIED",
                      "BLOCKED",
                      "CONFLICTED"
                    ]
                  },
                  "depends_on": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "blocks": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "conflicts_with": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "critical_path": {
                    "type": "boolean"
                  },
                  "wave_number": {
                    "type": "number"
                  }
                },
                "required": [
                  "id"
                ],
                "additionalProperties": false
              }
            },
            "edges": {
              "type": "object",
              "properties": {
                "dependencies": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minItems": 2
                  }
                },
                "conflicts": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minItems": 2
                  }
                },
                "satisfies": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minItems": 2
                  }
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "quick_summary": {
          "type": "object",
          "properties": {
            "started": {
              "type": "string"
            },
            "completed": {
              "type": "string"
            },
            "files_changed": {
              "type": "number"
            },
            "tests_added": {
              "type": "number"
            }
          },
          "additionalProperties": false
        }
      },
      "required": [
        "feature",
        "phase"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}